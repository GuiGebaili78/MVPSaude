@model Fiap.Web.MVPSaude.Models.UsuarioModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Usuario - Consultar</title>
</head>
<body>
    <h1>Usuario - Consultar</h1>

    <div class="form-horizontal">
        <hr />       

        <div class="form-group">
            <label><b>Nome</b></label>
            <div class="col-md-10">
                <span>@Model.NomeUsuario</span>
            </div>
        </div>

        <div class="form-group">
            <label class="col-md-2"><b>Senha</b></label>
            <div class="col-md-10">
                <span>@Model.SenhaUsuario</span>
            </div>
        </div>

        <div class="form-group">
            <label><b>Email</b></label>
            <div class="col-md-10">
                <span>@Model.EmailUsuario</span>
            </div>
        </div>

        
        <hr />
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <a class="button" asp-controller="Usuario" asp-action="Index" class="btn btn-warning">Voltar</a>
            </div>
        </div>
        <hr />
    </div>

</body>
</html>
